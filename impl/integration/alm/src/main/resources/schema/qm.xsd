	<xs:element name="alias">
      <xs:annotation>
         <xs:documentation xmlns:xml="http://www.w3.org/XML/1998/namespace" xml:lang="en">[DEPRECATED] [READ-ONLY] Element containing an alternate identifier for the resource.</xs:documentation>
      </xs:annotation>
	  <xs:complexType>
	    <xs:simpleContent>
	      <xs:extension base="xs:string">
