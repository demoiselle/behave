									<xs:annotation>
										<xs:documentation xmlns:xml="http://www.w3.org/XML/1998/namespace" xml:lang="en">[DEPRECATED] [READ-ONLY]</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>	
						<xs:element maxOccurs="unbounded" minOccurs="0" name="workitem">
							<xs:annotation>
								<xs:documentation xmlns:xml="http://www.w3.org/XML/1998/namespace" xml:lang="en">[READ-ONLY] A work item associated with the test script.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="rel" type="xs:string">
