								<xs:attribute ref="rdf:resource">
									<xs:annotation>
										<xs:documentation xmlns:xml="http://www.w3.org/XML/1998/namespace" xml:lang="en">[DEPRECATED] [READ-ONLY] The Jazz Team Server (JTS) URL of the contributor that authored the remote script.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
						</xs:complexType>
						</xs:element>
						<xs:element maxOccurs="1" minOccurs="0" name="ownerid">
							<xs:annotation>
								<xs:documentation xmlns:xml="http://www.w3.org/XML/1998/namespace" xml:lang="en">[DEPRECATED] The contributor that owns the remote script.</xs:documentation>
							</xs:annotation>							
							<xs:complexType mixed="true">
								<xs:annotation>
									<xs:documentation xmlns:xml="http://www.w3.org/XML/1998/namespace" xml:lang="en">[DEPRECATED] The Jazz Team Server (JTS) user ID of the contributor that owns the remote script.</xs:documentation>
								</xs:annotation>
								<xs:attribute ref="rdf:resource">
									<xs:annotation>
										<xs:documentation xmlns:xml="http://www.w3.org/XML/1998/namespace" xml:lang="en">[DEPRECATED] [READ-ONLY] The Jazz Team Server (JTS) URL of the contributor that owns the remote script.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
 						</xs:element>
					 	<xs:element maxOccurs="1" minOccurs="0" ref="customAttributes">
