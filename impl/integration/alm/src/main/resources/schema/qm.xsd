								<xs:attribute name="href" type="xs:anyURI" use="required">
									<xs:annotation>
										<xs:documentation xmlns:xml="http://www.w3.org/XML/1998/namespace" xml:lang="en">The URL of the datapool.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
						<xs:element maxOccurs="1" minOccurs="0" ref="customAttributes">
							<xs:annotation>
								<xs:documentation xmlns:xml="http://www.w3.org/XML/1998/namespace" xml:lang="en">User defined custom attribute name/value data on this artifact.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element maxOccurs="1" minOccurs="0" ref="esignatures">
						</xs:element>
                        <xs:element maxOccurs="unbounded" minOccurs="0" name="category">
							<xs:annotation>
								<xs:documentation xmlns:xml="http://www.w3.org/XML/1998/namespace" xml:lang="en">Category terms are provided in an attribute as is the value for that term. The category is used for query support and for sortable table views.</xs:documentation>
