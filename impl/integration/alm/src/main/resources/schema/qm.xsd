        			</xs:annotation>
       			</xs:element>
				<xs:element maxOccurs="1" minOccurs="0" name="message" type="xs:string">
					<xs:annotation>
            			<xs:documentation xmlns:xml="http://www.w3.org/XML/1998/namespace" xml:lang="en">[READ-ONLY] Error message.</xs:documentation>
        			</xs:annotation>
       			</xs:element>
				<xs:element maxOccurs="1" minOccurs="0" name="trace" type="xs:string">
					<xs:annotation>
            			<xs:documentation xmlns:xml="http://www.w3.org/XML/1998/namespace" xml:lang="en">[READ-ONLY] Error trace.</xs:documentation>
        			</xs:annotation>
       			</xs:element>
			</xs:sequence>
		</xs:complexType>
